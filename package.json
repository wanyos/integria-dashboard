{
  "name": "integria-dashboard",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve:express": "cross-env NODE_ENV=development node --watch server/app.js",
    "serve:test": "cross-env NODE_ENV=test node server/app.js",
    "connect-db": "cross-env NODE_ENV=development node server/db/mysql.js",
    "serve:db": "npm run connect-db & npm run serve:express",
    "serve:db:test": "npm run connect-db & npm run serve:test",
    "dev:open": "vite --port 5173 --open --mode development",
    "serve:dummy": "npm run serve:db & npm run dev:open",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "cypress open",
    "test:headless": "cypress run --headless --browser chrome open",
    "test:electron": "cypress run",
    "test:all": "npm run serve:db:test & npm run dev & npm run test:headless",
    "test:all-electron": "npm run serve:db:test & npm run dev & npm run test:electron",
    "__test:e2e__": "start-server-and-test preview http://localhost:5173 'cypress run --e2e'",
    "__test:e2e:dev__": "start-server-and-test 'vite dev --port 5173' http://localhost:5173 'cypress open --e2e'",
    "__test:cypress__": "npx cypress run",
    "lint": "eslint . --fix",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "apexcharts": "^4.3.0",
    "bcrypt": "^5.1.1",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "execSync": "^1.0.2",
    "json-2-csv": "^5.5.8",
    "mysql2": "^3.12.0",
    "nodemailer": "^6.10.0",
    "oh-vue-icons": "^1.0.0-rc3",
    "pinia": "^2.3.0",
    "radix-vue": "^1.9.12",
    "vue": "^3.5.13",
    "vue-cookies": "^1.8.5",
    "vue-echarts": "^7.0.3",
    "vue-loading-overlay": "^6.0.6",
    "vue-router": "^4.5.0",
    "vue3-apexcharts": "^1.8.0",
    "vue3-toastify": "^0.2.8",
    "vuejs3-datepicker": "^1.1.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.24",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/test-utils": "^2.4.6",
    "cross-env": "^7.0.3",
    "cypress": "^13.17.0",
    "eslint": "^9.18.0",
    "eslint-plugin-cypress": "^4.1.0",
    "eslint-plugin-vue": "^9.32.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.4.2",
    "start-server-and-test": "^2.0.9",
    "vite": "^5.4.11",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vitest": "^2.1.8"
  }
}
